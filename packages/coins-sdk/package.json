{
  "name": "@zoralabs/coins-sdk",
  "version": "0.4.0",
  "repository": "https://github.com/ourzora/zora-protocol",
  "license": "MIT",
  "type": "module",
  "main": "./dist/index.cjs",
  "module": "./dist/index.js",
  "types": "./dist/index.d.ts",
  "sideEffects": false,
  "files": [
    "src/",
    "dist",
    "README.md",
    "CHANGELOG.md"
  ],
  "exports": {
    ".": {
      "types": "./dist/index.d.ts",
      "import": "./dist/index.js",
      "default": "./dist/index.cjs"
    }
  },
  "scripts": {
    "build": "pnpm tsup",
    "build:js": "pnpm run build",
    "prepack": "pnpm build",
    "test:js": "vitest src",
    "test:integration": "vitest test-integration",
    "prettier:write": "prettier --write 'src/**/*.ts' 'test-integration/**/*.ts'",
    "lint": "prettier --check 'src/**/*.ts' 'test-integration/**/*.ts'",
    "api-codegen": "openapi-ts"
  },
  "dependencies": {
    "@hey-api/client-fetch": "^0.8.3",
    "@zoralabs/protocol-deployments": "workspace:^"
  },
  "peerDependencies": {
    "abitype": "^1.0.8",
    "viem": "^2.21.55"
  },
  "devDependencies": {
    "@hey-api/openapi-ts": "^0.64.10",
    "@lavamoat/preinstall-always-fail": "2.0.0",
    "@types/node": "^20.13.0",
    "@types/semver": "^7.5.8",
    "@zoralabs/tsconfig": "workspace:^",
    "graphql-prettier": "^1.0.6",
    "multiformats": "^13.2.0",
    "prettier": "^3.0.3",
    "prettier-plugin-solidity": "^1.1.3",
    "semver": "^7.6.3",
    "tsup": "^8.4.0",
    "typescript": "^5.2.2",
    "vite": "^4.5.0",
    "vitest": "^2.0.5"
  },
  "engines": {
    "node": ">=22"
  }
}
