# Tenderly Configuration
TENDERLY_ACCOUNT=zora
TENDERLY_PROJECT=nouns
TENDERLY_ACCESS_KEY=your_access_key_here
TENDERLY_DEPLOYER_PRIVATE_KEY=your_private_key_here

# Tenderly Testnet
TESTNET_ID=4b4374a9-4836-4845-94b0-5ca09d2d97c8
TENDERLY_VIRTUAL_TESTNET_RPC_URL=https://rpc.vnet.tenderly.co/devnet/4b4374a9-4836-4845-94b0-5ca09d2d97c8

# Development Deployments
# Set to true for development deployments on Base (uses addresses/{chainId}_dev.json)
# Only used for Base mainnet deployments, not Tenderly virtual testnets
DEV=false

PRIVATE_KEY=your_private_key_here
# DEPLOYER is used when mining deterministic salts - it must match the address that will deploy
DEPLOYER=address_of_deployer_associated_with_private_key

# Turnkey Configuration (for production deployments)
TURNKEY_API_PUBLIC_KEY=your_turnkey_api_public_key
TURNKEY_API_PRIVATE_KEY=your_turnkey_api_private_key
TURNKEY_ORGANIZATION_ID=your_turnkey_organization_id
TURNKEY_PRIVATE_KEY_ID=your_turnkey_private_key_id
TURNKEY_TARGET_ADDRESS=your_turnkey_target_address

# CRITICAL: For Turnkey deployments, DEPLOYER must equal TURNKEY_TARGET_ADDRESS
# The mined salts encode the deployer address, so only that address can deploy the contracts